@startuml
!include style.puml

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant  "<<class>>\n :Appointment" as Appointment MODEL_COLOR
participant "appt:Appointment" as newAppointment MODEL_COLOR
end box

Model -> Appointment : getPriority()
activate Appointment
Model <-- Appointment : priority
deactivate Appointment

create newAppointment
Model -> newAppointment: withPriority()
activate newAppointment
Model <-- newAppointment
deactivate newAppointment

@enduml
